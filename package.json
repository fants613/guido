{
  "name": "guido",
  "version": "3.8.1",
  "description": "基于webpack3的一键式开发工具，集成handlebars、雪碧图、SVG Sprite、自定义资源注入等方式",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zuzucheFE/guido.git"
  },
  "bugs": {
    "url": "https://github.com/zuzucheFE/guido/issues"
  },
  "keywords": [
    "guido",
    "webpack",
    "framework",
    "tools",
    "frontend"
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha test/index.js",
    "test:cover": "rm -rf ./coverage && ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/index.js --report html -- -R spec",
    "travis:test": "npm run cover",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/index.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "license": "MIT",
  "bin": {
    "guido": "./bin/guido.js"
  },
  "engines": {
    "node": ">=4.3.0 <5.0.0 || >=5.10"
  },
  "main": "lib/guido.js",
  "dependencies": {
    "@babel/core": "7.0.0-beta.47",
    "autoprefixer": "8.6.5",
    "babel-eslint": "8.2.6",
    "babel-loader": "8.0.0-beta.4",
    "babel-preset-zuzuche": "1.1.12",
    "bluebird": "3.5.1",
    "chalk": "2.4.1",
    "commander": "2.16.0",
    "css-loader": "0.28.11",
    "del": "3.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-react": "7.10.0",
    "extend": "3.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "glob": "7.1.2",
    "handlebars": "4.0.11",
    "handlebars-loader": "1.7.0",
    "happypack": "4.0.1",
    "html-webpack-plugin-for-multihtml": "2.30.2",
    "image-webpack-loader": "4.3.1",
    "node-sass": "4.9.3",
    "postcss-iconfont": "1.0.1",
    "postcss-loader": "2.1.6",
    "postcss-sprites": "4.2.1",
    "prompt": "1.0.0",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "svg-sprite-loader": "3.8.0",
    "svg-url-loader": "2.3.2",
    "svgo": "1.0.5",
    "svgo-loader": "2.1.0",
    "text-table": "0.2.0",
    "url-loader": "1.0.1",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.2",
    "webpack-parallel-uglify-plugin": "1.1.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "eslint": "4.19.1",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0"
  }
}
